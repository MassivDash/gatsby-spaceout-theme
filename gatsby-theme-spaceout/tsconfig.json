{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "lib": ["dom", "es5","es6", "es7", "scripthost", "es2015"],
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "downlevelIteration": true,
      "strictNullChecks": true,
      "noImplicitAny": false,
      "removeComments": true,
      "jsx": "react",
      "baseUrl": "./",
      "paths": {
        "*": [
          "node_modules/@types/*",
          "*"
        ],
        "@components/*": ["src/components/*"],
        "@icons/*": ["src/icons/*"],
        "@styles/*": ["src/styles/*"],
        "@utils/*": ["src/utils/*"],
        "@utils": ["src/utils/index.ts"],
        "@types/*": ["src/types/*"],
        }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  